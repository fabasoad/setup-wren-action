{
  "name": "setup-wren-action",
  "version": "1.0.13",
  "description": "This GitHub action installs wren",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fabasoad/setup-wren-action.git"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint:md": "markdownlint --config .markdownlint.yml '**/*.md'",
    "lint:es": "eslint --ext ts src",
    "lint": "yarn run lint:md && yarn run lint:es",
    "postinstall": "yarn husky install",
    "prepare": "yarn run snyk-protect",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "snyk-protect": "snyk protect",
    "test": "jest --config=jest.config.json --coverage"
  },
  "keywords": [
    "actions",
    "scripting-language",
    "programming-language",
    "wren",
    "wren-cli"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "homepage": "https://github.com/fabasoad/setup-wren-action#readme",
  "dependencies": {
    "@actions/core": "1.5.0",
    "@actions/tool-cache": "1.7.1",
    "glob": "7.1.7",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/glob": "7.1.4",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.10",
    "@types/sinon": "10.0.4",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "@vercel/ncc": "0.31.1",
    "chai": "4.3.4",
    "eslint": "7.32.0",
    "eslint-config-google": "0.14.0",
    "git-branch-is": "4.0.0",
    "husky": "7.0.2",
    "jest": "27.2.4",
    "jest-circus": "27.1.0",
    "markdownlint-cli": "0.28.1",
    "mocha-param": "2.0.1",
    "sinon": "11.1.2",
    "snyk": "1.697.0",
    "ts-jest": "27.0.5",
    "typescript": "4.4.2"
  },
  "snyk": true
}
