{
  "name": "setup-wren-action",
  "version": "1.0.0",
  "description": "This GitHub action installs wren",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fabasoad/setup-wren-action.git"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint": "eslint --ext ts src",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "test": "yarn run security:test && jest --config=jest.config.json --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "actions",
    "scripting-language",
    "programming-language",
    "wren",
    "wren-cli"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "homepage": "https://github.com/fabasoad/setup-wren-action#readme",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/tool-cache": "1.6.1",
    "glob": "7.1.6",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/glob": "7.1.3",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.7",
    "@types/sinon": "9.0.8",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "@vercel/ncc": "0.25.1",
    "chai": "4.2.0",
    "eslint": "7.13.0",
    "eslint-config-google": "0.14.0",
    "git-branch-is": "4.0.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "mocha-param": "2.0.1",
    "sinon": "9.2.1",
    "snyk": "1.427.0",
    "ts-jest": "26.4.4",
    "typescript": "4.0.5"
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "git-branch-is -r \"^((?!main).)*$\" && npm run build && git add dist/index.js",
      "pre-push": "npm run lint && npm test"
    }
  }
}
