{
  "name": "setup-wren-action",
  "version": "1.0.16",
  "description": "This GitHub action installs wren",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fabasoad/setup-wren-action.git"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint:md": "markdownlint --config .markdownlint.yml '**/*.md'",
    "lint:es": "eslint --ext ts src",
    "lint": "yarn run lint:md && yarn run lint:es",
    "postinstall": "yarn husky install",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "test": "jest --config=jest.config.json --coverage"
  },
  "keywords": [
    "actions",
    "scripting-language",
    "programming-language",
    "wren",
    "wren-cli"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "homepage": "https://github.com/fabasoad/setup-wren-action#readme",
  "dependencies": {
    "@actions/core": "1.8.2",
    "@actions/tool-cache": "2.0.1",
    "glob": "8.0.3",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/glob": "7.2.0",
    "@types/jest": "28.1.1",
    "@types/node": "17.0.41",
    "@types/sinon": "10.0.11",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "@vercel/ncc": "0.34.0",
    "chai": "4.3.6",
    "eslint": "8.17.0",
    "eslint-config-google": "0.14.0",
    "git-branch-is": "4.0.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-circus": "28.1.1",
    "markdownlint-cli": "0.31.1",
    "mocha-param": "2.0.1",
    "sinon": "14.0.0",
    "snyk": "1.947.0",
    "ts-jest": "28.0.4",
    "typescript": "4.7.3"
  },
  "snyk": true
}
